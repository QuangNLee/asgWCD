<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>CreateFoodController</servlet-name>
        <servlet-class>manager.food.assignment.controller.CreateFoodController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateFoodController</servlet-name>
        <url-pattern>/admin/food/create</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ListFoodController</servlet-name>
        <servlet-class>manager.food.assignment.controller.ListFoodController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListFoodController</servlet-name>
        <url-pattern>/admin/food/list</url-pattern>
    </servlet-mapping>
</web-app>